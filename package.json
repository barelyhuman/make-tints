{
	"name": "make-tints",
	"version": "0.0.7",
	"packageManager": "pnpm@6.32.3",
	"description": "[![NPM version](https://img.shields.io/npm/v/make-tints?color=a1b858&label=)](https://www.npmjs.com/package/make-tints)",
	"keywords": [
		"color",
		"tones",
		"tints",
		"make-tints"
	],
	"homepage": "https://github.com/barelyhuman/make-tints#readme",
	"bugs": {
		"url": "https://github.com/barelyhuman/make-tints/issues"
	},
	"license": "MIT",
	"author": "Reaper <ahoy@barelyhuman.dev>",
	"repository": "git+https://github.com/barelyhuman/make-tints.git",
	"funding": "https://github.com/sponsors/barelyhuman",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"directories": {
		"test": "test"
	},
	"scripts": {
		"build": "rimraf dist && unbuild && nr size",
		"dev": "unbuild --stub",
		"prepublishOnly": "nr build",
		"release": "bumpp --commit --push --tag && pnpm publish",
		"start": "esno src/index.ts",
		"test": "vitest",
		"size": "sizesnap",
		"fix": "prettier --write .",
		"typecheck": "tsc --noEmit"
	},
	"prettier": "@barelyhuman/prettier-config",
	"dependencies": {
		"@barelyhuman/tocolor": "^0.0.8"
	},
	"sizesnap": {
		"files": [
			"./dist/*"
		]
	},
	"devDependencies": {
		"@antfu/ni": "^0.13.2",
		"@babel/types": "^7.17.0",
		"@barelyhuman/prettier-config": "^0.1.0",
		"@types/node": "^17.0.23",
		"bumpp": "^7.1.1",
		"esno": "^0.14.1",
		"pnpm": "^6.32.3",
		"prettier": "^2.6.2",
		"rimraf": "^3.0.2",
		"sizesnap": "^0.1.1",
		"typescript": "^4.6.2",
		"unbuild": "^0.7.0",
		"vite": "^2.8.6",
		"vitest": "^0.7.10"
	}
}
